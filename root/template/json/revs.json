[% NOWRAPPER = 1 %]
[% USE JSON %]
[% i = 1 %]
[% FOREACH paste IN json %]
	[% paste.title = i %]
        [% IF paste.user_id == NULL %]
                [% name = "N/A" %]
        [% ELSE %]
                [% name = c.model("Paste::user").find({id => paste.user_id}).username %]
                [% IF name == NULL %]
                        [% name = "N/A" %]
                [% END %]
        [% END %]
        [% paste.user_id = name %]
	[% i = i + 1 %]
[% END %]
[% json.json %]

