[% NOWRAPPER = 1 %]
[% USE JSON %]
[% i = 0 %]
[% FOREACH paste IN json %]
	[% paste.ind = i %]
	[% rev = c.model("Paste::revision").find({revision_id => paste.id}) %]
	[% IF rev %]
		[% paste.revnum = rev.version %]
	[% ELSE %]
		[% paste.revnum = "Original" %]
	[% END %]
        [% IF paste.user_id == NULL %]
                [% name = "N/A" %]
        [% ELSE %]
                [% name = c.model("Paste::user").find({id => paste.user_id}).username %]
                [% IF name == NULL %]
                        [% name = "N/A" %]
                [% END %]
        [% END %]
        [% paste.user_id = name %]
	[% i = i + 1 %]
[% END %]
[% json.json %]

